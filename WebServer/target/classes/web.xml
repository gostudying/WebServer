	<servlet>
		<servlet-name>HelloServlet</servlet-name>
		<servlet-class>com.lanjian.servlet.impl.HelloServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HelloServlet</servlet-name>
		<url-pattern>/hello</url-pattern>
	</servlet-mapping>
</web-app>